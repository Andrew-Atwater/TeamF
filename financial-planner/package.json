    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "firebase": "^11.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-firebase-hooks": "^5.1.1",
    "react-router-dom": "^7.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "util": "^0.12.5",
    "web-streams-polyfill": "^4.1.0",
    "web-vitals": "^2.1.4"
